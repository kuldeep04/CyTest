{
  "name": "cytest",
  "version": "1.0.0",
  "description": "Reliable and straightforward UI automation with CyTest. Leverage the power of Cypress for your testing needs",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "runWithCucumber": "npx cypress run --spec cypress/integration/testExamples/BDD/ecommerce.feature",
    "e2e:chrome": "npx cypress run --browser chromium --reporter cypress-mochawesome-reporter",
    "e2e:firefox": "npx cypress run --browser firefox --reporter cypress-mochawesome-reporter",
    "runOnCloud": "npx cypress run --record --key c5851555-e28f-4ec3-aa67-88a39279759a --parallel",
    "e2e": "npx cypress run --spec \"cypress/integration/testExamples/end2end.js\" --reporter cypress-mochawesome-reporter"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "output": "cypress/cucumberReports/results.json"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^15.7.1",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-on-fix": "^1.1.0",
    "cypress-sql-server": "^1.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "multiple-cucumber-html-reporter": "^3.9.3"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.0.0",
    "@cypress/browserify-preprocessor": "latest",
    "neat-csv": "^5.2.0"
  }
}
